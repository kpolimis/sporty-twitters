	note = {{PMID:} 18954267},
	keywords = {Forecasting, Humans, Influenza, Human, Internet, Orthomyxoviridae, Pneumonia, Population Surveillance, Sentinel Surveillance, Statistics as Topic, United States},
	pages = {1443--1448}
},

