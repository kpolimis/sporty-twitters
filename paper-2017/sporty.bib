	keywords = {Adult, Aged, Aged, 80 and over, Autobiography as Topic, Baltimore, Catholicism, Clergy, Emotions, Female, Follow-Up Studies, Humans, Longevity, Longitudinal Studies, Population Surveillance, Reproducibility of Results, Retrospective Studies, Survival Rate, Wisconsin},
