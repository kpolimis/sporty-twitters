	keywords = {Adult, Angina Pectoris, Coronary Disease, Humans, Male, Middle Aged, Myocardial Infarction, Prognosis, Prospective Studies, Risk Factors, Self Concept, Type A Personality, Verbal Behavior},
	pages = {137--144}
}

